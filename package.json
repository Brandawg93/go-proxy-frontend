    "@uidotdev/usehooks": "^2.4.1",
    "@uiw/react-codemirror": "^4.23.7",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ansi-to-html": "^0.7.2",
    "geist": "^1.3.1",
    "http-status-codes": "^2.3.0",
    "loglevel": "^1.9.2",
